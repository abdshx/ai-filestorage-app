{
  //"extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["node", "node-fetch"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
